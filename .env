# Bot configuration
BOT_TOKEN=your_bot_token_here
ADMINS=123456789,987654321  # Comma-separated list of admin user IDs
CHANNEL_ID=-1001234567890  # Private channel ID where files will be stored

# Force subscription (set to 0 to disable)
FORCE_SUB=0  # or channel username without @ (e.g., "wleaksOwner")

# Auto delete media after X minutes (0 to disable)
AUTO_DELETE_TIME=0

# URL Shortener (optional)
URL_SHORTENER_API=
URL_SHORTENER_DOMAIN=

# Database URL (for future use with PostgreSQL)
DATABASE_URL=
